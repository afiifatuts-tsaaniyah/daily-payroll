     /* START DELETE */
     public function del($id)
     {
          $stQuery  = "DELETE FROM ".$this->table." ";      
          $stQuery .= "WHERE ts_id = ".$this->db->escapeString($id)."";
          $db->query($stQuery); 
     }

     function Proses()
     {
          $stQuery = "SELECT a.*,b.monthly,daily ,c.emp_position ,c.marital_status FROM tr_timesheet a , mt_salary b, mt_biodata_01 c WHERE a.biodata_id=b.biodata_id AND a.biodata_id=c.biodata_id AND a.month_process = '".$this->db->escapeString($this->monthProcess)."' AND a.year_process = '".$this->db->escapeString($this->yearProcess)."' AND a.start_date = '".$this->db->escapeString($this->startDate)."'";
          $query = $this->db->query($stQuery)->getResultArray();
          return $query;
     }

     function ProsesDB()
     {
          $stQuery = "SELECT a.*,b.monthly,daily ,c.emp_position ,c.marital_status FROM tr_timesheet a , mt_salary b, mt_biodata_01 c WHERE a.biodata_id=b.biodata_id AND a.biodata_id=c.biodata_id AND a.month_process = '".$this->db->escapeString($this->monthProcess)."' AND a.year_process = '".$this->db->escapeString($this->yearProcess)."' AND a.start_date = '".$this->db->escapeString($this->startDate)."'AND a.payroll_group = '".$this->db->escapeString($this->payrollGroup)."'";
          echo $stQuery;
          exit();
          $query = $this->db->query($stQuery)->getResultArray();
          return $query;
     }

     function PrintDB()
     {
          $stQuery = "SELECT a.*,b.*,c.*,d.*,e.tax_no,join_date,end_date,f.*,g.bank_name FROM tr_timesheet a , tr_slip b, tr_overtime c, mt_salary d, mt_biodata_01 e, tr_timesheet f, mt_bank g WHERE a.ts_id=b.ts_id AND a.ts_id = f.ts_id AND a.biodata_id=d.biodata_id AND a.ts_id=c.roster_id AND a.biodata_id = e.biodata_id AND a.bank_id = g.bank_code AND a.month_process = '".$this->db->escapeString($this->monthProcess)."' AND a.year_process = '".$this->db->escapeString($this->yearProcess)."' AND a.start_date = '".$this->db->escapeString($this->startDate)."'AND a.payroll_group = '".$this->db->escapeString($this->payrollGroup)."'";
          // echo $stQuery;
          // exit();
          $query = $this->db->query($stQuery)->getResultArray();
          return $query;
          // $this->db->query($query); 
          // return $query;
     }

     function getSM()
     {
          $stQuery = "SELECT * FROM tr_slip WHERE year_period = '".$this->db->escapeString($this->yearProcess)."' GROUP BY payroll_group;";
          // echo $stQuery;
          // exit();
          $query = $this->db->query($stQuery)->getResultArray();
          return $query;
     }

     function getBiodataIdBySM($payrollGroup)
     {
          $stQuery = "SELECT a.*,b.*,c.*,d.* FROM tr_slip a, mt_biodata_01 b, mt_salary c, mt_bank d WHERE a.biodata_id = b.biodata_id AND a.biodata_id = c.biodata_id AND a.bank_id = d.bank_code AND a.year_period = '".$this->db->escapeString($this->yearProcess)."' AND a.payroll_group = '".$payrollGroup."' GROUP BY a.biodata_id;";
          // echo $stQuery;
          // exit();
          $query = $this->db->query($stQuery)->getResultArray();
          return $query;
     }

     function getDataByBiodataId($bioId,$SM)
     {
          $stQuery = "SELECT * FROM tr_slip WHERE year_period = '2022' AND payroll_group = '".$SM."' AND biodata_id = '".$bioId."';";
          // echo $stQuery;
          // exit();
          $query = $this->db->query($stQuery)->getResultArray();
          return $query;
     }

     function Proses2()
     {
          $stQuery = "SELECT a.*,b.monthly,daily ,c.emp_position ,c.marital_status, FROM tr_timesheet a , mt_salary b, mt_biodata_01 c WHERE a.biodata_id=b.biodata_id AND a.biodata_id=c.biodata_id AND a.month_process = '".$this->db->escapeString($this->monthProcess)."' AND a.year_process = '".$this->db->escapeString($this->yearProcess)."'";
          $query = $this->db->query($stQuery)->getResultArray();
          return $query;
     }

     function Summary()
     {
          $stQuery = "SELECT * FROM tr_slip a LEFT JOIN mt_salary c ON a.biodata_id=c.biodata_id LEFT JOIN mt_bank d ON c.bank_id=d.bank_id WHERE a.month_period = '".$this->db->escapeString($this->monthProcess)."' AND a.year_period = '".$this->db->escapeString($this->yearProcess)."' AND a.payroll_group = '".$this->db->escapeString($this->payrollGroup)."' AND a.dept = '".$this->db->escapeString($this->dept)."'";
          $query = $this->db->query($stQuery)->getResultArray();
         /* return $query;*/
          // echo $stQuery;
          // exit();
          return $query;
     }
     function Summary1()
     {
          $stQuery = "SELECT * FROM tr_slip a LEFT JOIN mt_salary c ON a.biodata_id=c.biodata_id LEFT JOIN mt_bank d ON c.bank_id=d.bank_id WHERE a.month_period = '".$this->db->escapeString($this->monthProcess)."' AND a.year_period = '".$this->db->escapeString($this->yearProcess)."' AND a.payroll_group = '".$this->db->escapeString($this->payrollGroup)."'";
          $query = $this->db->query($stQuery)->getResultArray();
         /* return $query;*/
          // echo $stQuery;
          // exit();
          return $query;
     }
     function Print1()
     {
          $stQuery = "SELECT a.*,b.*,c.*,d.*,e.* FROM tr_timesheet a , tr_slip b, tr_overtime c, mt_salary d, mt_biodata_01 e WHERE a.ts_id=b.ts_id AND a.biodata_id=d.biodata_id AND a.ts_id=c.roster_id AND a.biodata_id = e.biodata_id AND a.month_process = '".$this->db->escapeString($this->monthProcess)."' AND a.year_process = '".$this->db->escapeString($this->yearProcess)."' ";
          $query = $this->db->query($stQuery)->getResultArray();
         /* return $query;*/
          
          return $query;
          /*return $query;*/
     }
     function getAllData()
     {
          $stQuery = "SELECT slip_id,full_name,dept,position FROM tr_slip WHERE month_period = '".$this->db->escapeString($this->monthProcess)."' AND year_period = '".$this->db->escapeString($this->yearProcess)."'";
          $query = $this->db->query($stQuery)->getResultArray();
          return $query;
     }
     function formlist()
     {
          $stQuery = "SELECT COUNT(*) AS total_karyawan,SUM(totalgaji) AS total FROM tr_slip WHERE month_period = '".$this->db->escapeString($this->monthProcess)."' AND year_period = '".$this->db->escapeString($this->yearProcess)."' ";
          // echo $stQuery;
          // exit();
          $query = $this->db->query($stQuery)->getResultArray();
          return $query;
     }
     function invoice()
     {
          $stQuery = "SELECT dept,COUNT(*) AS totalempl,SUM(wdtotal) AS totalwork,SUM(gajipokok) AS gajipokok, SUM(totalot) AS overtime, SUM(tunjangan) AS tunjangan FROM tr_slip WHERE month_period = '".$this->db->escapeString($this->monthProcess)."' AND year_period = '".$this->db->escapeString($this->yearProcess)."' AND payroll_group = '".$this->db->escapeString($this->payrollGroup)."' GROUP BY dept ";
          // echo $stQuery;
          $query = $this->db->query($stQuery)->getResultArray();
          return $query;
     }
     function Print($ts_id)
     {
          $stQuery = "SELECT a.*,b.*,c.*,d.*,e.tax_no FROM tr_timesheet a , tr_slip b, tr_overtime c, mt_salary d, mt_biodata_01 e WHERE a.ts_id=b.ts_id AND a.biodata_id=d.biodata_id AND a.ts_id=c.roster_id AND a.biodata_id = e.biodata_id AND a.ts_id = '".$this->db->escapeString($ts_id)."'";
           //echo $stQuery;
          //exit();
          $query = $this->db->query($stQuery)->getRowArray();
          return $query;
          // $this->db->query($query); 
          // return $query;
     }
     function baca()
     {
          $stQuery = "SELECT b.*,a.start_date FROM tr_slip b, tr_timesheet a WHERE a.ts_id=b.ts_id AND a.month_process = '".$this->db->escapeString($this->monthProcess)."' AND a.year_process = '".$this->db->escapeString($this->yearProcess)."' AND a.start_date = '".$this->db->escapeString($this->startDate)."'";
          $query = $this->db->query($stQuery)->getResultArray();
          return $query;
     }

     function getSlipById()
     {
          $stQuery = "SELECT * FROM tr_slip WHERE slip_id = ".$this->db->escapeString($slipId)."";
          $query = $this->db->query($stQuery)->getResultArray();
          return $query;
     }
     /* END DELETE */
     /* START GET ALL DATA */
     public function getAll()
     {
          $stQuery  = "SELECT * FROM ".$this->table." "; 
          $query  = $this->db->query($stQuery); 
          $arrayList = $query->getResultArray(); 
          return $arrayList; 
     }
     /* END GET ALL DATA */
     /* START GET DATA BY ID */
     public function getById($id)
     {
          $stQuery  = "SELECT * FROM ".$this->table." ";     
          $stQuery .= "WHERE ts_id = ".$this->db->escapeString($id)."";
          $query  = $this->db->query($stQuery); 
          $arrayRow = $query->getRowArray(); 
          return $arrayRow; 
     }
     /* END GET DATA BY ID */
     /* START GET OBJECT DATA BY ID */
     public function setObjectById($id)
     {
