          $this->d04 = $arrayRow['d04']; 
          $this->d05 = $arrayRow['d05']; 
          $this->d06 = $arrayRow['d06']; 
          $this->d07 = $arrayRow['d07']; 
          $this->d08 = $arrayRow['d08']; 
          $this->d09 = $arrayRow['d09']; 
          $this->d10 = $arrayRow['d10']; 
          $this->d11 = $arrayRow['d11']; 
          $this->d12 = $arrayRow['d12']; 
