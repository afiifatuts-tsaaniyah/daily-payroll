     public function deleteByIdPeriod($monthProcess, $yearProcess, $startDate)
      {
         $st  = "DELETE FROM ".$this->table." ";
         $st .='WHERE month_process ='.'"'.$monthProcess.'"';
         $st .='AND year_process ='.'"'.$yearProcess.'"';
         $st .='AND start_date ='.'"'.$startDate.'"';
         $this->db->query($st);
      }
