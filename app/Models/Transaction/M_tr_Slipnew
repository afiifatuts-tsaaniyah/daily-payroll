          $stQuery .=   'jk ='.$this->jk.','; 
          $stQuery .=   'jkk ='.$this->jkk.','; 
          $stQuery .=   'jkm ='.$this->jkm.','; 
          $stQuery .=   'jp ='.$this->jp.','; 
          $stQuery .=   'jht ='.$this->jht.','; 
          $stQuery .=   'emp_bpjs ='.$this->empBpjs.','; 
          $stQuery .=   'emp_jp ='.$this->empJp.','; 
          $stQuery .=   'emp_jht ='.$this->empJht.','; 
          $stQuery .=   'non_tax_allowance ='.$this->nonTaxAllowance.','; 
          $stQuery .=   'ptkp_total ='.$this->ptkpTotal.','; 
          $stQuery .=   'irregular_tax ='.$this->irregularTax.','; 
          $stQuery .=   'regular_tax ='.$this->regularTax.','; 
          $stQuery .=   "salary_status ='".$this->db->escapeString($this->salaryStatus)."', "; 
          $stQuery .=   "status_remarks ='".$this->db->escapeString($this->statusRemarks)."', "; 
          $stQuery .=   'gross_earn ='.$this->grossEarn.','; 
          $stQuery .=   "pic_edit ='".$this->db->escapeString($this->picEdit)."', "; 
          $stQuery .=   "edit_time ='".$this->db->escapeString($this->editTime)."', "; 
          $stQuery .=   "pic_input ='".$this->db->escapeString($this->picInput)."', "; 
          $stQuery .=   "input_time ='".$this->db->escapeString($this->inputTime)."' "; 
          $stQuery .= 'WHERE '; 
          $stQuery .=   'slip_id = '.$this->db->escapeString($id).''; 
          return $stQuery; 
     }

     function getSlipById($slipId)
     {
          $stQuery = "SELECT * FROM tr_slip WHERE slip_id = ".$this->db->escapeString($slipId)."";
          $query = $this->db->query($stQuery)->getRowArray();
          /*echo $stQuery;
          exit();*/
          return $query;

     }
     /* END QUERY UPDATE */
     /* START DELETE */
     public function del($id)
     {
          $stQuery  = "DELETE FROM ".$this->table." ";      
          $stQuery .= "WHERE slip_id = ".$this->db->escapeString($id)."";
          $db->query($stQuery); 
     }
     /* END DELETE */
     /* START GET ALL DATA */
     public function getAll()
     {
          $stQuery  = "SELECT * FROM ".$this->table." "; 
          $query  = $this->db->query($stQuery); 
          $arrayList = $query->getResultArray(); 
          return $arrayList; 
     }
     /* END GET ALL DATA */
     /* START GET DATA BY ID */
     public function getById($id)
     {
          $stQuery  = "SELECT * FROM ".$this->table." ";     
          $stQuery .= "WHERE slip_id = ".$this->db->escapeString($id)."";
          $query  = $this->db->query($stQuery); 
          $arrayRow = $query->getRowArray(); 
          return $arrayRow; 
     }
     /* END GET DATA BY ID */
     /* START GET OBJECT DATA BY ID */
